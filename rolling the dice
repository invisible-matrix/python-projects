import random
import time

rollagain="yes"

while rollagain == "yes":
  print("\ndice is rolling................................")
  time.sleep(2)

  dice1 = random.randint(1,6)
  dice2 = random.randint(1,6)

  print("the values are :")
  print("the dice1 is :",dice1,"dice2 is :",dice2)

  if dice1==dice2 :
    print("double")
  else :
    print("keep try") 
  rollagain = input("enetr yes or no to ")   

  

     
